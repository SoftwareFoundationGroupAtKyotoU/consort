gcd(y1, y2) {
  if y1 <= 0 then
    0
  else if y2 <= 0 then
    0
  else if y1 = y2 then
    y1
  else if y1 > y2 then
    gcd((y1 - y2), y2)
  else
    gcd(y1, (y2 - y1))
}

{
  let m = _ in
  let n = _ in {
    $gamma {
	  m: ~ > 0 int;
	  n: ~ > 0 int
	};
	let z = gcd(m, n) in
	assert(z >= 1)
  }
}
